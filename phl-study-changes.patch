+function pTrapBarrage(mSide)
+	delay = getPerfectDelayDM(THICKNESS) * 3.7
+		
+	cBarrage(mSide)
+	t_wait(delay * 3)
+	wallSAdj(mSide, 1.9)
+
+	t_wait(delay * 2.5)
